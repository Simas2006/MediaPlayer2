{
  "expo": {
    "name": "mp2-mobile-app",
    "description": "Mobile app for MP2",
    "slug": "mp2-mobile-app",
    "privacy": "public",
    "sdkVersion": "29.0.0",
    "platforms": ["ios","android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
